{
  "mcpServers": {
    "isaac-sim": {
      "disabled": false,
      "timeout": 60,
      "type": "stdio",
      "command": "/path/to/isaac-sim-mcp/mcp_venv/bin/python",
      "args": [
        "/path/to/isaac-sim-mcp/isaac_mcp/server.py"
      ]
    },
    "ros-mcp-server": {
      "disabled": false,
      "timeout": 60,
      "type": "stdio",
      "command": "bash",
      "args": [
        "-c",
        "source /opt/ros/humble/setup.bash && /path/to/ros-mcp-server/.venv/bin/python /path/to/ros-mcp-server/server.py"
      ]
    },
    "Prompts": {
      "disabled": false,
      "timeout": 60,
      "type": "stdio",
      "command": "/path/to/mcp-server-example/.venv/bin/python",
      "args": [
        "/path/to/ros-mcp-server/prompt.py"
      ]
    },
    "Resources": {
      "disabled": false,
      "timeout": 60,
      "type": "stdio",
      "command": "/path/to/mcp-server-example/.venv/bin/python",
      "args": [
        "/path/to/ros-mcp-server/resource.py"
      ]
    },
    "todo": {
      "disabled": true,
      "timeout": 60,
      "type": "stdio",
      "command": "python3",
      "args": [
        "/path/to/todo-list-mcp/server.py"
      ]
    }
  }
}

